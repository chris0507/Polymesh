            "name": "blockHash",
            "type": "Hash",
            "isOptional": true
        },
        {
            "name": "did",
            "type": "IdentityId",
            "isOptional": false
        },
        {
            "name": "buffer_time",
            "type": "u64",
            "isOptional": true
        }],
        "type": "CddStatus"
    },
    {
        "name": "getAssetDid",
        "description": "function is used to query the given ticker DID",
        "params": [{
            "name": "blockHash",
            "type": "Hash",
            "isOptional": true
        },
        {
            "name": "ticker",
            "type": "Ticker",
            "isOptional": false
        }],
        "type": "AssetDidResult"
    },
    {
        "name": "getDidRecords",
        "description": "Used to get the did record values for a given DID",
        "params": [{
            "name": "blockHash",
            "type": "Hash",
            "isOptional": true
        },
        {
            "name": "did",
            "type": "IdentityId",
            "isOptional": false
        }],
        "type": "DidRecords"
    }],
    "mips":[{
        "name": "getVotes",
